{% extends 'base.html.twig' %}

{% block title %}Delivery{% endblock %}

{% block body %}
    <h1>Delivery</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ delivery.id }}</td>
            </tr>
            <tr>
                <th>Number</th>
                <td>{{ delivery.number }}</td>
            </tr>
            <tr>
                <th>Delivery_date</th>
                <td>{{ delivery.deliveryDate ? delivery.deliveryDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Notices</th>
                <td>{{ delivery.notices }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('delivery_index') }}">back to list</a>

    <a href="{{ path('delivery_edit', {'id': delivery.id}) }}">edit</a>

    {{ include('delivery/_delete_form.html.twig') }}
{% endblock %}
